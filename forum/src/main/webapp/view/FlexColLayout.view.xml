<mvc:View
	xmlns="sap.f"
	id="flexColLayoutView"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:m="sap.m"
	displayBlock="true"
	controllerName="dhbw.mosbach.neuendorf03.forum.controller.FlexColLayout"
	height="100%">
		<FlexibleColumnLayout
			id="fcl"
			busy="{appView>/busy}"
			busyIndicatorDelay="{appView>/delay}"
			stateChange="onStateChanged"
			layout="{/layout}"
			backgroundDesign="Solid">
		</FlexibleColumnLayout>
</mvc:View>